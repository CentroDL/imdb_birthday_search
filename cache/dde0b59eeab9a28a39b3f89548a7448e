                    <button class="btn small" value="Yes" name="ynb_rw3857078_yes" onclick="CS.TMD.user_review_vote('rw3857078', 'tt1520211', 'yes', 'true');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_rw3857078_no" onclick="CS.TMD.user_review_vote('rw3857078', 'tt1520211', 'no', 'true');" >No</button>
